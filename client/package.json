{
  "name": "robotlab-x",
  "version": "0.9.0",
  "description": "A cross platform desktop application for creating and running robots (UI side)",
  "author": "GroG",
  "license": "MPL-2.0",
  "scripts": {
    "react-start": "react-scripts start",
    "move-client": "mv build ../server/dist/client || move build ../server/dist/client",
    "react-build": "react-scripts build && yarn run move-client",
    "test": "react-scripts test --env=jsdom",
    "test-no-watch": "react-scripts test --env=jsdom --watchAll=false",
    "test-coverage": "react-scripts test --coverage --env=jsdom --watchAll=false",
    "start": "cross-env BROWSER=none yarn run react-start"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/serviceWorker.ts",
      "!<rootDir>/src/App.tsx",
      "!<rootDir>/src/Routes.tsx",
      "!src/**/types.ts",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "./",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.24.1",
